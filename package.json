    "proptypes": "^1.0.0",
    "react-markdown": "^4.0.3",
    "serve": "^6.0.0",
    "styled-components": "^3.4.4"
  },
  "main": "webpack.config.babel.js",
  "directories": {
    "test": "__test__",
    "lib": "lib"
  }
}
